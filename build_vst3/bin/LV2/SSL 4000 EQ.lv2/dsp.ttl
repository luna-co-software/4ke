plug:hpf_freq
	a lv2:Parameter ;
	rdfs:label "HPF Frequency" ;
	rdfs:range atom:Float ;
	lv2:default 20 ;
	lv2:minimum 20 ;
	lv2:maximum 500 .

plug:lpf_freq
	a lv2:Parameter ;
	rdfs:label "LPF Frequency" ;
	rdfs:range atom:Float ;
	lv2:default 20000 ;
	lv2:minimum 3000 ;
	lv2:maximum 20000 .

plug:lf_gain
	a lv2:Parameter ;
	rdfs:label "LF Gain" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum -20 ;
	lv2:maximum 20 .

plug:lf_freq
	a lv2:Parameter ;
	rdfs:label "LF Frequency" ;
	rdfs:range atom:Float ;
	lv2:default 100 ;
	lv2:minimum 20 ;
	lv2:maximum 600 .

plug:lf_bell
	a lv2:Parameter ;
	rdfs:label "LF Bell Mode" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1	 ;
	lv2:portProperty lv2:enumeration , lv2:toggled ;
	lv2:scalePoint [
		rdfs:label "Off" ;
		rdf:value 0 ;
	], [
		rdfs:label "On" ;
		rdf:value 1 ;
	] .

